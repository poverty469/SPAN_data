{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./examples/Source.example.json",
  "type": "object",
  "title": "Source",
  "description": "A bibliographic source.",
  "required": ["title", "year", "url"],
  "properties": {
    "title": {
      "type": "string",
      "description": "Presentable name",
      "examples": ["U.S. Census Bureau, American Community Survey"]
    },
    "year": {
      "type": "integer",
      "description": "Year the data was published.",
      "examples": [2017]
    },
    "url": {
      "type": "string",
      "description": "URL address of the source or publisher.",
      "examples": ["www.url.com"]
    }
  }
}
